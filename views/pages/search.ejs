<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <% include ../partials/search-bar %>
    <div class="container">
      <ul class="list-group" id="search-group">
        <% for (var item=0; item < catagories.length ; item++) { %>
          <a href="./catagories/<%= catagories[item].cat_id %>">
            <li class="list-group-item" >
              <%= catagories[item].cat_name %>
            </li>
          </a>
        <% } %>
      </ul>
    </div>
  </body>
</html>
